{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080/",
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/ibm-plex-mono": "^4.5.13",
    "@hookform/resolvers": "^2.9.10",
    "@opengovsg/design-system-react": "^0.1.0-alpha.7",
    "@tanstack/react-query": "^4.24.2",
    "class-validator": "^0.14.0",
    "framer-motion": "^8.5.4",
    "inter-ui": "^3.19.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-router-dom": "^6.8.0",
    "wretch": "^2.4.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@snyk/protect": "latest",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/mdx2-csf": "^0.0.4",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "*",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.6.10",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "react-app-alias-ex": "^2.1.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "overrides": {
    "react-refresh": "0.14.0"
  },
  "scriptComments": {
    "build": [
      "The flags are required for the app to build successfully.",
      "`CI=false` ensures that warnings in the app linter are not treated as errors (preventing a build)",
      "`INLINE_RUNTIME_CHUNK=false` prevents inline scripts from appearing in the build output. This is to prevent blank pages due to possible strict CSP rules on the backend"
    ]
  },
  "scripts": {
    "gen:theme-typings": "chakra-cli tokens src/theme/index.ts",
    "postinstall": "npm run gen:theme-typings",
    "dev": "npm start",
    "start": "react-app-rewired start",
    "build": "cross-env CI=false INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "test": "react-app-rewired test --passWithNoTests",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint ./ --ignore-path .gitignore && prettier . -c",
    "lint:fix": "eslint ./ --ignore-path .gitignore --fix && prettier . -c --write",
    "pre-commit": "lint-staged",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true,
  "jest": {
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(react-markdown|)).+\\.(js|jsx|mjs|cjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "identity-obj-proxy",
      "@fontsource/ibm-plex-mono": "identity-obj-proxy",
      "react-markdown": "<rootDir>/__mocks__/react-markdown.tsx"
    }
  }
}
